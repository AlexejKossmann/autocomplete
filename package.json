{
  "name": "autocomplete",
  "version": "0.1.0",
  "description": "autocomplete js library written in typescript",
  "main": "dist/autocomplete.js",
  "scripts": {
    "dev": "nodemon src/autocomplete.ts",
    "build": "tsc -p .",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-ts\" \"npm run dev\""
  },
  "repository": {
    "type": "git"
  },
  "author": "Alexej Kossmann",
  "license": "GNU GPLv3",
  "bugs": {
    "url": "https://github.com/AlexejKossmann/autocomplete/issues"
  },
  "homepage": "https://github.com/AlexejKossmann/autocomplete#readme",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
